#include <stdio.h>
int p(int c)
{
    if (c == 0) return 1;
    else if (c == 1) return 1;
    else return p(c - 1) +  p(c - 2);
}
int main()
{
    int a;
    int b;
    int sum = 0;
    int odd = 0;
    int even = 0;
    //printf("값을 입력하세요");
    scanf("%d", &a);
    int n = 10;
    for(int i = 0 ; i < a; i++) {
        printf("%d ", p(i));
        sum = sum + p(i);
        if (p(i) % 2 == 0) {
            even +=1;
        } else if(p(i) % 2 == 1) {
            odd +=1;
        }
    }
    printf("\n합 : %d", sum);
    printf("\n홀/짝 %d / %d", odd, even);
    
    return 0;
}
